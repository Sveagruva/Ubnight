{
  "manifest_version": 2,
  "version": "0.1.2.3",
  "name": "Ubnight",
  "description": "endless night",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["main.js"],
    "css" : ["darkScreen_ubnight.css"],
    "run_at": "document_start",
    "all_frames": true
  }],
  "browser_action":{
    "default_popup": "popup.html",
    "browser_style": false
  },
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "16": "icons/16.png"
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "default_locale": "en",
  "homepage_url": "https://github.com/sveagruva/"
}